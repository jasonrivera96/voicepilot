{
  "expo": {
    "name": "Voice Pilot",
    "slug": "voicepilot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/voicito.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loading.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alpalasaul.voicepilot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/voicito.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alpalasaul.voicepilot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "77816c03-7940-4a4f-a7cb-7d246fa94708"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
